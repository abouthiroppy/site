{
  "name": "site",
  "main": ".",
  "author": "abouthiroppy <git@about-hiroppy.com http://about-hiroppy.com",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.css",
    "test": "nyc ava",
    "type-check": "tsc --noEmit",
    "start": "parcel index.html",
    "build": "NODE_ENV=production parcel build index.html",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "tap": true,
    "failFast": true,
    "concurrency": 5
  },
  "devDependencies": {
    "@types/react": "16.8.11",
    "@types/react-dom": "16.8.3",
    "@types/scrollreveal": "0.0.4",
    "@types/styled-components": "4.1.12",
    "ava": "0.25.0",
    "babel-preset-react": "6.24.1",
    "conventional-changelog-cli": "1.3.22",
    "eslint": "4.19.1",
    "eslint-config-sky": "1.6.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "nyc": "11.9.0",
    "parcel-bundler": "1.12.3",
    "postcss": "6.0.23",
    "postcss-advanced-variables": "1.2.2",
    "postcss-cssnext": "3.1.0",
    "postcss-for": "2.1.1",
    "postcss-mixins": "6.2.1",
    "postcss-modules": "1.4.1",
    "postcss-random": "1.0.9",
    "postcss-smart-import": "0.7.6",
    "prettier": "1.16.4",
    "stylelint": "8.4.0",
    "stylelint-config-sky": "1.1.0",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "github-calendar": "^1.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "scrollreveal": "^3.3.6",
    "styled-components": "^4.1.3",
    "styled-reset": "^1.7.0"
  }
}
