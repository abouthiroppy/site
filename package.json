{
  "name": "site",
  "main": ".",
  "author": "abouthiroppy <git@about-hiroppy.com http://about-hiroppy.com",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.css",
    "test": "nyc ava",
    "type-check": "tsc --noEmit",
    "start": "parcel index.html",
    "build": "NODE_ENV=production parcel build index.html",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "tap": true,
    "failFast": true,
    "concurrency": 5
  },
  "devDependencies": {
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.0.10",
    "@types/scrollreveal": "0.0.4",
    "@types/styled-components": "^4.1.10",
    "ava": "^0.25.0",
    "babel-preset-react": "^6.24.1",
    "conventional-changelog-cli": "^1.3.4",
    "eslint": "^4.10.0",
    "eslint-config-sky": "^1.6.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "nyc": "^11.3.0",
    "parcel-bundler": "^1.11.0",
    "postcss": "^6.0.13",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^6.2.0",
    "postcss-modules": "^1.3.0",
    "postcss-random": "^1.0.9",
    "postcss-smart-import": "^0.7.6",
    "prettier": "^1.13.7",
    "stylelint": "^8.2.0",
    "stylelint-config-sky": "^1.1.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "github-calendar": "^1.3.1",
    "normalize.css": "^8.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "scrollreveal": "^3.3.6",
    "styled-components": "^4.1.3"
  }
}
