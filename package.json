{
  "name": "site",
  "main": ".",
  "author": "abouthiroppy <git@about-hiroppy.com http://about-hiroppy.com",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.css",
    "test": "nyc ava",
    "start": "sweetpack watch",
    "build": "NODE_ENV=production sweetpack build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "tap": true,
    "failFast": true,
    "concurrency": 5
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "conventional-changelog-cli": "^1.3.4",
    "eslint": "^4.10.0",
    "eslint-config-sky": "^1.6.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "nyc": "^11.3.0",
    "postcss": "^6.0.13",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^6.2.0",
    "postcss-random": "^1.0.9",
    "postcss-reporter": "^5.0.0",
    "postcss-smart-import": "^0.7.5",
    "react-hot-loader": "^3.1.1",
    "stylelint": "^8.2.0",
    "stylelint-config-sky": "^1.1.0",
    "sweetpack": "^0.10.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "github-calendar": "^1.2.0",
    "normalize.css": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "scrollreveal": "^3.3.6"
  }
}
